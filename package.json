{
  "name": "finance",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "build:fe": "remix vite:build",
    "dev": "remix vite:dev",
    "process-sql": "node scripts/process-sql-template.js",
    "dev:docker": "npm run process-sql && docker compose -f docker-compose.dev.yml down --volumes && docker volume rm finance_postgres-data 2>/dev/null || true && docker compose -f docker-compose.dev.yml up --build",
    "prod:docker": "npm run process-sql && docker compose -f docker-compose.yml down --volumes && docker volume rm finance_postgres-data 2>/dev/null || true && docker compose -f docker-compose.yml up --build",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "set-port": "node scripts/set-port.js",
    "start": "npm run set-port && node server.js",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "db:migrate": "node --loader ts-node/esm node_modules/.bin/knex migrate:latest",
    "db:seed": "node --loader ts-node/esm node_modules/.bin/knex seed:run",
    "db:ensure-demo": "node scripts/direct-demo-data.js",
    "db:rollback": "node --loader ts-node/esm node_modules/.bin/knex migrate:rollback",
    "docker:psql": "docker compose exec postgres psql -U ${DB_USER:-finance} -d ${DB_NAME:-finance}"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@remix-run/express": "^2.16.2",
    "@remix-run/node": "^2.16.0",
    "@remix-run/react": "^2.16.0",
    "@remix-run/serve": "^2.16.0",
    "@tanstack/react-query": "^5.71.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.16",
    "argon2": "^0.41.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "framer-motion": "^12.6.2",
    "isbot": "^4.1.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.477.0",
    "pg": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "recharts": "^2.15.1",
    "remix-auth": "^4.1.0",
    "remix-auth-form": "^3.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prettier": "3.5.2",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
