{
  "name": "finance",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build && npm run db:migrate && npm run db:ensure-demo",
    "build:fe": "remix vite:build",
    "build:new": "chmod +x ./scripts/rebuild-docker.sh && ./scripts/rebuild-docker.sh",
    "dev": "remix vite:dev",
    "dev:start": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "dev:init-db": "DOCKER_ENV=true node scripts/dev-init-db.js",
    "dev:with-db": "./scripts/dev.sh",
    "dev:restart": "./scripts/restart-dev.sh",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "set-port": "node scripts/set-port.js",
    "start": "npm run set-port && node server.js",
    "start:with-db": "npm run set-port && node scripts/start-with-db.js",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "db:migrate": "node --loader ts-node/esm node_modules/.bin/knex migrate:latest",
    "db:seed": "node --loader ts-node/esm node_modules/.bin/knex seed:run",
    "db:ensure-demo": "node scripts/direct-demo-data.js",
    "db:rollback": "node --loader ts-node/esm node_modules/.bin/knex migrate:rollback",
    "db:init": "node scripts/direct-demo-data.js",
    "docker:migrate": "DOCKER_ENV=true npm run db:migrate",
    "docker:ensure-demo": "DOCKER_ENV=true npm run db:ensure-demo",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:restart": "docker-compose down && docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:shell": "docker-compose exec app sh",
    "docker:psql": "docker-compose exec postgres psql -U ${DB_USER:-finance} -d ${DB_NAME:-finance}"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@remix-run/express": "^2.16.2",
    "@remix-run/node": "^2.16.0",
    "@remix-run/react": "^2.16.0",
    "@remix-run/serve": "^2.16.0",
    "@types/bcryptjs": "^2.4.6",
    "argon2": "^0.41.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "framer-motion": "^12.6.2",
    "isbot": "^4.1.0",
    "knex": "^3.1.0",
    "lucide-react": "^0.477.0",
    "pg": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^4.1.0",
    "remix-auth-form": "^3.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prettier": "3.5.2",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
