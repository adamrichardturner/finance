# React Production Build in Development
# Using production build of React in development for better performance
NODE_ENV=development

# Disable React Dev Tools in development for better performance
REACT_DISABLE_DEVTOOLS=true 

# Skip unnecessary checks in development
REACT_SKIP_EXPENSIVE_CHECKS=true

# Add Vite specific optimizations
VITE_OPTIMIZE_DEPS=true
VITE_FORCE_OPTIMIZATION=true

# Speed up the build
VITE_FAST_BUILD=true
